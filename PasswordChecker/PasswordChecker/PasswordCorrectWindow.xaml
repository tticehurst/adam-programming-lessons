<Window x:Class="PasswordChecker.PasswordCorrectWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PasswordChecker"
        mc:Ignorable="d"
        Title="PasswordCorrectWindow" Height="450" Width="800" ResizeMode="NoResize">
    <Grid Background="DarkSlateGray">
        <Label x:Name="labelPasswordConfirmation" Content="Password correct!" HorizontalAlignment="Left" Margin="299,188,0,0" VerticalAlignment="Top" Width="161" Background="Gold" Foreground="Black">
            <Label.Effect>
                <DropShadowEffect/>
            </Label.Effect>
        </Label>
    </Grid>
</Window>
